NameVirtualHost 127.0.0.1:80

<VirtualHost 127.0.0.1:80>
	ServerName jquery-firehint.apache-local
	DocumentRoot /var/www/apache/jquery-fireHint/www

	<Directory />
		AllowOverride All
		Options -Indexes
	</Directory>
	
	ErrorLog /var/www/apache/jquery-fireHint/logs/error.log
	CustomLog /var/www/apache/jquery-fireHint/logs/access.log common
</VirtualHost>