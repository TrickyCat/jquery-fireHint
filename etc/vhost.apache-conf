NameVirtualHost 127.0.0.1:80

<VirtualHost 127.0.0.1:80>
	ServerName test.apache-local
	DocumentRoot /var/www/apache/test/www

	<Directory />
		AllowOverride All
		Options -Indexes
	</Directory>
	
	ErrorLog /var/www/apache/test/logs/error.log
	CustomLog /var/www/apache/test/logs/access.log common
</VirtualHost>